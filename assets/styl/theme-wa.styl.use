@import "./_theme.styl"
@import "./_theme_wood.styl"

body
  background: url("http://s3-ap-northeast-1.amazonaws.com/giji-assets/images/bg/nagatanienn.png")

palette_single(title-bar, grass, BROAD)
palette_single(icons,     grass, BROAD)

palette(world,  black)
palette(metal,  metal)

palette(says,   light)
