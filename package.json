  "scripts": {
    "dev": "yarn run cp-monaco && yarn run api && node server.js",
    "prod": "yarn run cp-monaco && yarn run api && yarn run styl && yarn run build",
    "w.styl": "ls assets/styl/*.styl.use | xargs stylus --compress --watch --sourcemap --out static/css/.",
    "cp-monaco": "rm -rf static/monaco-editor && cp -r node_modules/monaco-editor/min static/monaco-editor",
    "api": "webpack --config config/webpack/index-node.coffee",
    "styl": "ls assets/styl/*.styl.use | xargs stylus --compress --sourcemap --out static/css/. && ( rm static/css/*.css.gz || echo ) && gzip -9N static/css/*.css",
